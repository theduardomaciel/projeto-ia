# LLM API Keys
# Escolha o(s) provedor(es) que deseja usar

# Google Gemini (prioridade - API gratuita)
GEMINI_API_KEY=your_gemini_api_key_here

# DeepSeek (alternativa)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com

# Grok (xAI - alternativa)
GROK_API_KEY=your_grok_api_key_here
GROK_BASE_URL=https://api.x.ai/v1

# Llama (via Groq ou outro provedor)
LLAMA_API_KEY=your_llama_api_key_here
LLAMA_BASE_URL=https://api.groq.com/openai/v1

# Configurações do sistema
DEFAULT_LLM_PROVIDER=gemini  # gemini | deepseek | grok | llama
MAX_TOKENS=1000
TEMPERATURE=0.7
